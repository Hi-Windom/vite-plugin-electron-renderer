{
  "name": "vite-plugin-electron-renderer",
  "version": "0.0.2",
  "description": "Support use Node.js API in Electron-Renderer",
  "main": "index.mjs",
  "types": "types",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hi-Windom/vite-plugin-electron-renderer.git"
  },
  "author": "soltus <694357845@qq.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc --emitDeclarationOnly && vite build",
    "prepublishOnly": "npm run test && npm run build",
    "test": "vitest run"
  },
  "dependencies": {
    "lib-esm": "~0.3.0"
  },
  "devDependencies": {
    "esbuild": "^0.16.11",
    "rollup": "^3.8.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vite-plugin-utils": "^0.4.0",
    "vitest": "^0.28.3"
  },
  "files": [
    "types",
    "index.mjs",
    "index.js",
    "electron-renderer.js",
    "builtins"
  ],
  "keywords": [
    "vite",
    "plugin",
    "electron",
    "renderer"
  ]
}
